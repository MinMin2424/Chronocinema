<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="DarkOceanBrush" Color="{StaticResource DarkOcean}"/>
    <SolidColorBrush x:Key="PurpleBlueBrush" Color="{StaticResource PurpleBlue}"/>
    <SolidColorBrush x:Key="WhiteBrush" Color="{StaticResource White}"/>
    <SolidColorBrush x:Key="LightPurpleBrush" Color="{StaticResource LightPurple}"/>
    <SolidColorBrush x:Key="TransparentBrush" Color="{StaticResource Transparent}"/>
    <SolidColorBrush x:Key="CardBackgroundBrush" Color="{StaticResource CardBackground}"/>
    <SolidColorBrush x:Key="SelectedItemBrush" Color="{StaticResource SelectedItem}"/>

    <SolidColorBrush x:Key="CompletedBrush" Color="{StaticResource Completed}"/>
    <SolidColorBrush x:Key="WatchingBrush" Color="{StaticResource Watching}"/>
    <SolidColorBrush x:Key="PlanningBrush" Color="{StaticResource Planning}"/>
    <SolidColorBrush x:Key="PausedBrush" Color="{StaticResource Paused}"/>
    <SolidColorBrush x:Key="DroppedBrush" Color="{StaticResource Dropped}"/>

    <SolidColorBrush x:Key="GrayBrush" Color="{StaticResource Gray}"/>
    <SolidColorBrush x:Key="LightBlackBrush" Color="{StaticResource LightBlack}"/>

</ResourceDictionary>